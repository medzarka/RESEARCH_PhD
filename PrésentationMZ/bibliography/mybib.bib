@Article{Coutinho2012,
  Title                    = {Context Based Information Retrieval},
  Author                   = {Perpetual Coutinho, Fiona and Asnani, Kavita and Amos Caeiro, Daniel},
  Journal                  = {International Journal of Advanced Research in Computer Science and Electronics Engineering (IJARCSEE)},
  Year                     = {2012},
  Number                   = {7},
  Volume                   = {1},

  Abstract                 = {Despite the recent advances in search quality, the fast increase in the size of the Web collection has introduced new challenges for Web ranking algorithms. In fact, there are still many situations in which the users are presented with imprecise or very poor results. One of the key difficulties is the fact that users usually submit very short and ambiguous queries, and they do not fully specify their information needs. That is, it is necessary to improve the query formation process if better answers are to be provided. The proposed system is a revised concept-based query expansion technique, which allows disambiguating queries submitted to search engines. The concepts are extracted by analyzing log files and query relations mined using association rules. The concepts related to the current query are then shown to the user who selects the concept that he interprets is most related to his query. This concept is used to expand the original query and the expanded query is processed instead. Using a Web test collection of stock news articles this technique has been implemented and it can be clearly seen from the results that the proposed system leads to gains in precision. The proposed system attempts to improvise the performance of the vector space model system and provide relevant documents to the user based on the context.},
  ISSN                     = {2277–9043},
  Keywords                 = {Query expansion, Information retrieval system, Association rule mining.}
}

