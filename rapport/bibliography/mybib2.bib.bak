@InProceedings{Amit2006,
  Title                    = {Scalability of Multimedia Applications on Next-Generation Processors},
  Author                   = {Amit, G. and Caspi, Y. and Vitale, R. and Pinhas, A.T.},
  Booktitle                = {Multimedia and {Expo}, 2006 {IEEE} International Conference on},
  Year                     = {2006},
  Month                    = {July},
  Pages                    = {17-20},

  Abstract                 = {In the near future, the majority of personal computers are expected to have several processing units. This is referred to as core multiprocessing (CMP). Furthermore, each of the computation units will be capable of running multiple hardware threads. To benefit from the additional processing power, application developers should multithread their software. This paper studies the scalability (expected speedup factor) of multimedia applications and provides guidelines for proper utilization of these new multi-core platforms. In particular, we discuss the decomposition method, load balancing, synchronization primitives, interaction with the operating system and hardware issues such as cache hierarchy and memory bandwidth. Our results are based on analysis of several state-of-the-art applications, including H.264 video encoding, panoramic image stitching and dense optical-flow estimation. We demonstrate how to multithread them properly, and report scalability results on several next-generation multi-core platforms},
  Keywords                 = {code standards;image sequences;multi-threading;multimedia computing;multiprocessing systems;synchronisation;video coding;CMP;H.264 video encoding;core multiprocessing;decomposition method;load balancing;multimedia application;multithreading;next-generation processor;operating system;optical-flow estimation;panoramic image stitching;scalability;state-of-art application;synchronization primitive;Application software;Bandwidth;Guidelines;Hardware;Image analysis;Load management;Microcomputers;Operating systems;Scalability;Yarn}
}

